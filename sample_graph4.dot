digraph {
    nodesep=0.5
    rankdir=TB // グラフ全体を上から下（Top to Bottom）に設定
	A1; B1; C1; D1; E1;
	A2; B2; C2; D2; E2;
	A3; B3; C3; D3; E3;
	A4; B4; C4; D4; E4;
	A5; B5; C5; D5; E5;
	A1 -> B1 -> C1 -> D1 -> E1 [style=invis];
	A2 -> B2 -> C2 -> D2 -> E2 [style=invis];
	A3 -> B3 -> C3 -> D3 -> E3 [style=invis];
	A4 -> B4 -> C4 -> D4 -> E4 [style=invis];
	A5 -> B5 -> C5 -> D5 -> E5 [style=invis];
    // ノードの定義（自然な順番で定義）
    // 横方向に同じランクを配置
}
