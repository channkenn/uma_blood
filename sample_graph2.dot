digraph {
	nodesep=0.5 rankdir=TB  // グラフ全体を縦並びに設定

	// ノードの定義
	A1; A2; A3; A4; A5;
	A1 -> A2 -> A3 -> A4 -> A5;
	B1; B2; B3; B4; B5;
	C1; C2; C3; C4; C5;
	D1; D2; D3; D4; D5;
	E1; E2; E3; E4; E5;

	// 各ランクにノードを配置
	{rank = same; A1; B1; C1; D1; E1;}
	{rank = same; A2; B2; C2; D2; E2;}
	{rank = same; A3; B3; C3; D3; E3;}
	{rank = same; A4; B4; C4; D4; E4;}
	{rank = same; A5; B5; C5; D5; E5;}
}
